ALGORITHM cheackpoint
VAR
charact,word,vowel:INTEGER;
n:STRING;
tab:array of char[6];
BEGIN
  read(n) ;
  charact:=0
for charact from 0 to length(n)-1 do;
   charact:=charact+1;
END_FOR
word:=0
for word from 0 to length(n)-1 do;
IF (word[i]:=" ") THEN
word:=word+1;    
    END_FOR
END_IF
vowel:=0
tab:=["a","e","y","o","u","i"]
for vowel from 0 to length(n-1) do
IF ( vowel[i] in tab) THEN
    vowel:=vowel+1;
    ELSE
    vowel:=0,
END_IF
END_FOR
write (vowel);
write(word);
write(charact)
END