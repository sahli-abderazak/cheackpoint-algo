ALGORITHM cheackpoint
VAR
charact,word,vowel:INTEGER;
n:STRING;
BEGIN
  read(n) ;
  i:=0
for charact from 1 to length(n) do;
   i:=i+1;
   write(i)
END_FOR
j:=0
for word from 1 to length(n) do;
IF (j[i]:="") THEN
j:=j-1;    
ELSE
    j:=j+1;
    END_FOR
END_IF
k:=0
for vowel from 1 to length(n)do
IF ( k[i] in ['a','e','y','o','u','i']) THEN
    k:=k+1;
    ELSE
    k:=0,
END_IF
END_FOR
END